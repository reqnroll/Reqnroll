<Project TreatAsLocalProperty="TaskFolder;TaskAssembly">

  <Import Project="../../../Reqnroll.Tools.MsBuild.Generation\build\Reqnroll.Tools.MsBuild.Generation.props" />

  <PropertyGroup>
    <ReqnrollTestTFM Condition=" '$(MSBuildRuntimeType)' == 'Core'">$(Reqnroll_Core_Tools_TFM)</ReqnrollTestTFM>
    <ReqnrollTestTFM Condition=" '$(MSBuildRuntimeType)' != 'Core'">$(Reqnroll_FullFramework_Tools_TFM)</ReqnrollTestTFM>
  </PropertyGroup>
  
  <PropertyGroup>
    <ReqnrollTasksDir>$(MSBuildThisFileDirectory)../../../Reqnroll.Tools.MsBuild.Generation/bin/$(Configuration)/$(ReqnrollTestTFM)/</ReqnrollTasksDir>
  </PropertyGroup>

  <Import Project="../../../Reqnroll.Tools.MsBuild.Generation\build\Reqnroll.Tools.MsBuild.Generation.tasks"/>

</Project>
