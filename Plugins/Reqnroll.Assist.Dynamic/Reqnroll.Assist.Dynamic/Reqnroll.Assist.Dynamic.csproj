<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>netstandard2.0</TargetFrameworks>
    <AssemblyOriginatorKeyFile>$(ReqnrollKeyFile)</AssemblyOriginatorKeyFile>
    <SignAssembly>$(ReqnrollEnableStrongNameSigning)</SignAssembly>
    <PublicSign>$(ReqnrollPublicSign)</PublicSign>
    
    <PackageId>Reqnroll.Assist.Dynamic</PackageId>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <NuspecFile>$(MSBuildThisFileDirectory)Reqnroll.Assist.Dynamic.nuspec</NuspecFile>

    <ImplicitUsings>enable</ImplicitUsings>
    <LangVersion>10</LangVersion>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Dynamitey" Version="3.0.3" />
	</ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\Reqnroll\Reqnroll.csproj" />
  </ItemGroup>
</Project>
